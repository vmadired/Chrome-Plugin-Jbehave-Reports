{
  "manifest_version": 2,

  "name": "Create Defect",
  "description": "Create defect from jbehave reports",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
	"file:///*"
  ]
,
	"content_security_policy": "script-src 'self' ; object-src 'self'"
}
